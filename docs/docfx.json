{
  "metadata": [
    {
        "src": [
            {
                "files": [ "**.csproj" ],
                "src": "../src/Mawosoft.BenchmarkDotNetToolbox"
            }
        ],
        "dest": "api",
        "filter": "filter.yml",
        "disableGitFeatures": false,
        "disableDefaultFilter": false,
        "properties": {
            "TargetFramework": "netstandard2.0"
        }
    }
  ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadata": {
            "_appTitle": "BenchmarkDotNet Toolbox",
            "_appFooter": "Copyright &copy; 2021 Matthias Wolf, Mawosoft",
            "_enableSearch": true,
            "_enableNewTab": true,
            // Templates changed to affect contribution only, but not "view source".
            "_disableContribution": true
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/custom"
        ],
        "xref": [ "xrefmap-BenchmarkDotNet.yml" ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "postProcessors": [],
        "markdownEngineName": "dfm",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": true,
        "disableGitFeatures": false
    }
}
